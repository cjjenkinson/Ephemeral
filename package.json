{
  "name": "captionease-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preinstall": "node -e \"if (process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Use yarn for installing: https://yarnpkg.com/en/docs/install')\"",
    "start": "NODE_ENV=local STAGE=local ./bin/start.sh",
    "start:development": "NODE_ENV=development STAGE=development ./bin/start.sh",
    "start:graphql": "rm -rf ./node_modules/.cache/babel-loader && nodemon --ext \"js,json,ts,graphql,gql\" --inspect=0.0.0.0:9229 --exec babel-node --extensions \".js,.ts\" src/services/graphql/index.ts",
    "start:ci": "BYPASS_AUTH=true yarn run start",
    "server": "rm -rf ./node_modules/.cache/babel-loader && nodemon --ext \"js,json,ts,graphql,gql\" --inspect=0.0.0.0:9229 --exec babel-node --extensions \".js,.ts\" mock/server.ts",
    "build": "webpack",
    "cleanup": "rm -Rf build && mkdir -p build && rm -Rf .webpack",
    "zip": "yarn cleanup && yarn build && cd .webpack && node ../bin/handlers.js | while read i; do zip ../build/${i}.zip -r ${i}.js; done",
    "zip:server": "yarn generate && ./bin/build-server.sh",
    "eslint": "eslint --cache --cache-location=node_modules/.cache/.eslintcache --fix",
    "lint": "yarn run eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --list-different src/",
    "prettier:write": "prettier --write src/",
    "typecheck": "concurrently --raw yarn:typecheck:*",
    "typecheck:services": "tsc --noEmit",
    "generate-types": "graphql-schema-typescript generate-ts ./src/services/api/types --typePrefix \"\"",
    "generate": "yarn run generate:prisma && yarn run generate:nexus && yarn run generate:graphql-codegen",
    "generate:prisma": "prisma generate --schema ./src/services/graphql/prisma/schema.prisma",
    "generate:nexus": "cross-env GENERATE=true cross-env DOTENV_CONFIG_PATH=.env.example ts-node --require dotenv/config --transpile-only -P nexus.tsconfig.json src/services/graphql/schema/index.ts",
    "generate:graphql-codegen": "graphql-codegen",
    "prisma:migrate": "yarn prisma migrate dev --preview-feature --schema ./src/services/graphql/prisma/schema.prisma",
    "prisma:seed": "babel-node --extensions \".js,.ts\" ./src/test/seed/index.ts",
    "test": "jest --maxWorkers=50%",
    "test:ci": "jest --runInBand",
    "deploy:development": "AWS_PROFILE=<PROFILE-NAME>-development-admin STAGE=development AWS_ACCOUNT_ID=<ACCOUNT_ID> ./bin/deploy.sh",
    "deploy:core:development": "AWS_PROFILE=<PROFILE-NAME>-development-admin STAGE=development AWS_ACCOUNT_ID=<ACCOUNT_ID> ./bin/deploy-core.sh",
    "deploy:graphql:development": "AWS_PROFILE=<PROFILE-NAME>-development-admin STAGE=development AWS_ACCOUNT_ID=<ACCOUNT_ID> ./bin/deploy-graphql.sh",
    "deploy:audio:development": "AWS_PROFILE=<PROFILE-NAME>-development-admin STAGE=development AWS_ACCOUNT_ID=<ACCOUNT_ID> ./bin/deploy-services.sh AudioService",
    "deploy:production": "AWS_PROFILE=<PROFILE-NAME>-production-admin STAGE=production AWS_ACCOUNT_ID=<ACCOUNT_ID> ./bin/deploy.sh",
    "deploy:core:production": "AWS_PROFILE=<PROFILE-NAME>-production-admin STAGE=production AWS_ACCOUNT_ID=<ACCOUNT_ID> ./bin/deploy-core.sh",
    "deploy:notify": "babel-node ./bin/deploy-notify.js cameron",
    "run-script:development": "AWS_PROFILE=<PROFILE-NAME>-development-admin STAGE=development AWS_ACCOUNT_ID=<ACCOUNT_ID> NODE_ENV=development ./bin/run-script.sh"
  },
