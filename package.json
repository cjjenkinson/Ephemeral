  "jest": {
    "setupFiles": [
      "dotenv/config"
    ],
    "setupFilesAfterEnv": [
      "./src/services/graphql/test/jest-setup.ts"
    ],
    "transform": {
      "\\.tsx?$": [
        "babel-jest",
        {
          "configFile": "./.babelrc.jest.js"
        }
      ]
    }
  },
  "prisma": {
    "schema": "src/services/graphql/prisma/schema.prisma"
  },
  "oclif": {
    "commands": "./bin/mock/commands",
    "bin": "mock",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
